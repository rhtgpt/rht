-- (Turn on DBMS Output panel in your tool to see messages)
EXEC GD.DROP_INTERVAL_PART_SP('CIM_OFM_SMG6_LOGGING');





BEGIN
   FOR i IN 1..30 LOOP
      INSERT INTO GD.CIM_OFM_SMG6_LOGGING
        (TRACE_ID, ALIAS, AUDIT_CREATE_DATE, SMG3_LOGGING_DATA, FILTRATION_JSON_DATA)
      VALUES
        (SYS_GUID(),
         CASE MOD(i,2) WHEN 0 THEN 'ASIA' ELSE 'EU' END,  -- alternate alias
         TRUNC(SYSDATE) - i,                              -- past 30 days
         RPAD('S', 1024, 'S'),                            -- ~1 KB CLOB
         RPAD('F', 1024, 'F'));                           -- ~1 KB CLOB
   END LOOP;
   COMMIT;
END;
/
